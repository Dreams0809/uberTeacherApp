    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sign-Up</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
    <link rel="stylesheet" href="public/css/Sign-Up.css" type="text/css">
</head>
<body>
  
    <!-- Header -->
    <section id="header">

        <h1 class="logo">Uber<span>Professor</span></h1>
    </section>
    <!-- Header -->
    
    
    
    <section>
        <div class="sign-up-form">
            <h1>Sign-Up</h1>
